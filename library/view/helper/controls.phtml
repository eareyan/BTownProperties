<?php if ($this->pageCount): ?>
<div class="paginationControl">
<!-- Previous page link -->
<?php if (isset($this->previous)): ?>
    <a href="<?=Zend_Registry::get("ROOTPATH").$this->module_name.'/'.$this->contro_name.'/'.$this->action_name?>/page/<?=$this->previous?>">
    <!--<a href="<?php echo $this->url(array('page' => $this->previous)); ?>">-->
  &lt; Previous
</a> |
<?php else: ?>
<span class="disabled">&lt; Previous</span> |
<?php endif; ?>
<!-- Numbered page links -->
<?php foreach ($this->pagesInRange as $page): ?>
<?php if ($page != $this->current): ?>
    <a href="<?=Zend_Registry::get("ROOTPATH").$this->module_name.'/'.$this->contro_name.'/'.$this->action_name?>/page/<?=$page?>">
    <!--<a href="<?php echo $this->url(array('page' => $page)); ?>">-->
      <?php echo $page; ?>
  </a> |
<?php else: ?>
  <?php echo $page; ?> |
<?php endif; ?>
<?php endforeach; ?>
<!-- Next page link -->
<?php if (isset($this->next)): ?>
    <a href="<?=Zend_Registry::get("ROOTPATH").$this->module_name.'/'.$this->contro_name.'/'.$this->action_name?>/page/<?=$this->next?>">
    <!--<a href="<?php echo $this->url(array('page' => $this->next)); ?>">-->
  Next &gt;
</a>
<?php else: ?>
<span class="disabled">Next &gt;</span>
<?php endif; ?>
</div>
<?php endif; ?>